import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;

public class stringManipulator {

    public static void main(String[] args) throws IOException {

        BufferedReader reader = new BufferedReader(new InputStreamReader(System.in));
        String input = reader.readLine();

        String line;

        while (!"Done".equals(line = reader.readLine())) {
            String[] cmdArgs = line.split("\\s+");

            switch(cmdArgs[0]) {
                case "Change":
                    input = input.replaceAll(cmdArgs[1], cmdArgs[2]);
                    System.out.println(input);
                    break;
                case "Includes":
                    if (input.contains(cmdArgs[1])) {
                        System.out.println("True");
                    } else {
                        System.out.println("False");
                    }
                    break;
                case "End":
                    if (input.endsWith(cmdArgs[1])) {
                        System.out.println("True");
                    } else {
                        System.out.println("False");
                    }
                    break;
                case "Uppercase":
                    String upperCaseInput = input.toUpperCase();
                    System.out.println(upperCaseInput);
                    input = upperCaseInput;
                    break;
                case "FindIndex":
                    int index = input.indexOf(cmdArgs[1]);
                    System.out.println(index);
                    break;
                case "Cut":
                    int startingIndex = Integer.parseInt(cmdArgs[1]);
                    int length = Integer.parseInt(cmdArgs[2]);
                    input = input.substring(startingIndex, startingIndex + length);
                    System.out.println(input);
                    break;
            }
        }
    }
}
